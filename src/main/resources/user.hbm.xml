<!--在domain包下-->
<hibernate-mapping package="me.lovesasuna.bot.entity">

    <!--类名为User，表名也为User-->
    <class name="ynamiEntity" table="user">

        <!--主键映射，属性名为id，列名也为id-->
        <id name="id" column="id">
            <!--根据底层数据库主键自动增长-->
            <generator class="native"/>
        </id>

        <!--非主键映射，属性和列名一一对应-->
        <property name="upSet" column="upset"/>
        <property name="subscribeMap" column="subscribemap"/>
        <property name="time" column="time"/>
        <property name="intercept" column="intercept"/>
    </class>
</hibernate-mapping>